						"title": "Non Configured Component"
					},
					"Toolbar": {
						"iconClass": "ff-component"
					}
				}
			}
		},
		"Notepad": {
			"window": {
				"id": "Notepad",
				"native": true,
				"path": "notepad.exe",
				"url": "",
				"defaultHeight": 600,
				"autoShow": true,
				"alwaysOnTop": false,
				"resizable": true,
				"showTaskbarIcon": false,
				"contextMenu": true,
				"addToWorkspace": true
			},
			"component": {
				"spawnOnHotkey": [
					"ctrl",
					"alt",
					"n"
				]
			},
			"foreign": {
				"services": {
					"workspaceService": {
						"isArrangable": true
					}
				},
				"components": {
					"App Launcher": {
						"launchableByUser": true
					},
					"Window Manager": {
						"persistWindowState": false,
						"FSBLHeader": true
					},
					"Toolbar": {
						"iconURL": "$applicationRoot/assets/img/notepad.png"
					}
				}
			}
		},
		"Getting Started Tutorial": {
			"window": {
