        event.cancelable = true;\n    }\n\n    result = this.canvas.dispatchEvent(new CustomEvent(eventName, event));\n\n    if (cancelable) {\n        return result;\n    }\n}\n","name":"node_modules/fin-hypergrid/src/lib/events.js"}},"hash":"899d73813d3b512612d0c44106825a64","dependencyBlock":{"type":"DependenciesBlock","dependencies":[{"type":"ConstDependency","expression":"","range":[26,39],"loc":{"start":{"line":3,"column":0},"end":{"line":3,"column":13}}},{"type":"CommonJsRequireDependency","request":"object-iterators","range":[57,75],"loc":{"start":{"line":5,"column":8},"end":{"line":5,"column":35}}},{"type":"RequireHeaderDependency","range":[49,56],"loc":{"start":{"line":5,"column":8},"end":{"line":5,"column":35}}},{"type":"CommonJsRequireDependency","request":"../behaviors/Behavior","range":[102,125],"loc":{"start":{"line":7,"column":15},"end":{"line":7,"column":47}}},{"type":"RequireHeaderDependency","range":[94,101],"loc":{"start":{"line":7,"column":15},"end":{"line":7,"column":47}}}],"variables":[],"blocks":[]},"source":{"_cachedSource":{"source":{"type":"ReplaceSource","replacements":[[102,124,"43",3],[94,100,"__webpack_require__",4],[57,74,"40",1],[49,55,"__webpack_require__",2],[26,38,"",0]]},"hash":"899d73813d3b512612d0c44106825a64"}}}{"type":"NormalModule","constructor":{"request":"node_modules/fin-hypergrid/src/lib/fields.js","userRequest":"node_modules/fin-hypergrid/src/lib/fields.js","rawRequest":"fields","loaders":[],"resource":"node_modules/fin-hypergrid/src/lib/fields.js","parser":{"type":"Parser"}},"identifier":"node_modules/fin-hypergrid/src/lib/fields.js","assigned":{"issuer":"node_modules/fin-hypergrid/src/lib/DataSourceOrigin.js","useSourceMap":true},"build":{"built":true,"buildTimestamp":1533476244049,"cacheable":true,"meta":{},"assets":[],"fileDependencies":["node_modules/fin-hypergrid/src/lib/fields.js"],"contextDependencies":[],"strict":true,"warnings":[],"errors":[],"_source":{"type":"OriginalSource","value":"'use strict';\n\n/**\n *