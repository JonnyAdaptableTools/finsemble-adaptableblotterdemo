osition == \"left\") {\n\t\t\t\t//buttons.push(<FinsembleToolbarSeparator key={sectionPosition} />);\n\t\t\t}\n\n\t\t\tvar sectionComponent = React.createElement(\n\t\t\t\tFinsembleToolbarSection,\n\t\t\t\t{\n\t\t\t\t\tarrangeable: sectionPosition === \"center\",\n\t\t\t\t\tref: \"pinSection\",\n\t\t\t\t\tname: sectionPosition,\n\t\t\t\t\tpinnableItems: pinnableItems,\n\t\t\t\t\tclassName: sectionPosition,\n\t\t\t\t\tkey: sectionPosition,\n\t\t\t\t\thandleOverflow: sectionPosition === \"center\",\n\t\t\t\t\thandlePins: sectionPosition === \"center\" },\n\t\t\t\tbuttons\n\t\t\t);\n\t\t\tsections.push(sectionComponent);\n\t\t}\n\t\treturn sections;\n\t}\n\n\trender() {\n\t\tconsole.log(\"Toolbar Render \");\n\t\tif (!this.state.sections) return;\n\t\treturn React.createElement(\n\t\t\tFinsembleToolbar,\n\t\t\t{ onDragEnd: this.onPinDrag },\n\t\t\tthis.getSections()\n\t\t);\n\t}\n\n}\nFSBL.addEventListener(\"onReady\", function () {\n\tToolbarStore.initialize(function () {\n\t\tReactDOM.render(React.createElement(Toolbar, null), document.getElementById(\"toolbar_parent\"));\n\t});\n});","name":"node_modules/babel-loader/lib/index.js??ref--9!C:/code/finsemble-adaptableblotterdemo/src-built-in/components/toolbar/src/dynamicToolbar.jsx","sourceMap":{"version":3,"sources":["src-built-in\\components\\toolbar\\src\\dynamicToolbar.jsx"],"names":["React","ReactDOM","FinsembleToolbar","FinsembleButton","FinsembleToolbarSection","FinsembleToolbarSeparator","ToolbarStore","AutoArrange","AlwaysOnTop","BringToFront","MinimizeAll","WorkspaceLauncherButton","WorkspaceMenuOpener","Search","customComponents","pinnableItems","Toolbar","Component","constructor","props","state","sections","getSectionsFromMenus","finWindow","fin","desktop","Window","getCurrent","bindCorrectContext","onSectionsUpdate","bind","onPinDrag","err","response","setState","value","componentDidMount","console","log","bringToFront","componentWillMount","self","setupPinnedHotKeys","data","pin","refs","pinSection","ele