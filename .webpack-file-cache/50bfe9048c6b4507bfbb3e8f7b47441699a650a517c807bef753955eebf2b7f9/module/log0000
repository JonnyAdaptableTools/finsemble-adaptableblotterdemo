 smaller bundles)\n\t\t\tif(typeof item[0] !== \"number\" || !alreadyImportedModules[item[0]]) {\n\t\t\t\tif(mediaQuery && !item[2]) {\n\t\t\t\t\titem[2] = mediaQuery;\n\t\t\t\t} else if(mediaQuery) {\n\t\t\t\t\titem[2] = \"(\" + item[2] + \") and (\" + mediaQuery + \")\";\n\t\t\t\t}\n\t\t\t\tlist.push(item);\n\t\t\t}\n\t\t}\n\t};\n\treturn list;\n};\n\nfunction cssWithMappingToString(item, useSourceMap) {\n\tvar content = item[1] || '';\n\tvar cssMapping = item[3];\n\tif (!cssMapping) {\n\t\treturn content;\n\t}\n\n\tif (useSourceMap && typeof btoa === 'function') {\n\t\tvar sourceMapping = toComment(cssMapping);\n\t\tvar sourceURLs = cssMapping.sources.map(function (source) {\n\t\t\treturn '/*# sourceURL=' + cssMapping.sourceRoot + source + ' */'\n\t\t});\n\n\t\treturn [content].concat(sourceURLs).concat([sourceMapping]).join('\\n');\n\t}\n\n\treturn [content].join('\\n');\n}\n\n// Adapted from convert-source-map (MIT)\nfunction toComment(sourceMap) {\n\t// eslint-disable-next-line no-undef\n\tvar base64 = btoa(unescape(encodeURIComponent(JSON.stringify(sourceMap))));\n\tvar data = 'sourceMappingURL=data:application/json;charset=utf-8;base64,' + base64;\n\n\treturn '/*# ' + data + ' */';\n}\n","name":"node_modules/css-loader/lib/css-base.js"}},"hash":"bd40a55d763cebf813868c0a998d9264","dependencyBlock":{"type":"DependenciesBlock","dependencies":[],"variables":[],"blocks":[]},"source":{"_cachedSource":{"source":{"type":"ReplaceSource","replacements":[]},"hash":"bd40a55d763cebf813868c0a998d9264"}}}{"type":"NormalModule","constructor":{"request":"node_modules/css-loader/index.js!src-built-in/components/assets/css/_globals.css","userRequest":"node_modules/css-loader/index.js!src-built-in/components/assets/css/_globals.css","rawRequest":"-!../../../node_modules/css-loader/index.js!../assets/css/_globals.css","loaders":[{"loader":"node_modules/css-loader/index.js"}],"resource":"src-built-in/components/assets/css/_globals.css","parser":{"type":"Parser"}},"identifier":"no