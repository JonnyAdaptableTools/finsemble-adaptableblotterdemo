{
	"comment": "Core services loaded into the finsemble module",
	"services": {
		"launcherService": {
			"visible": false,
			"showDevConsoleOnVisible": true,
			"active": true,
			"name": "launcherService",
			"html": "$servicesRoot/launcher/launcher.html",
			"file": "$servicesRoot/launcher/launcherService.js",
			"category": "system"
		},
		"workspaceService": {
			"visible": false,
			"showDevConsoleOnVisible": true,
			"active": true,
			"name": "workspaceService",
			"html": "$servicesRoot/workspace/workspace.html",
			"file": "$servicesRoot/workspace/workspaceService.js",
			"category": "system"
		},
		"storageService": {
			"visible": false,
			"showDevConsoleOnVisible": true,
			"active": true,
			"name": "storageService",
			"html": "$servicesRoot/storage/storage.html",
			"file": "$servicesRoot/storage/storageService.js",
			"category": "system"
		},
		"linkerService": {
			"visible": false,
			"showDevConsoleOnVisible": true,
			"active": true,
			"name": "linkerService",
			"html": "$servicesRoot/linker/linker.html",
			"file": "$servicesRoot/linker/linkerService.js",
			"category": "system",
			"channels": [
				{
					"name": "group1",
					"color": "#8781BD",
					"border": "#797381"
				},
				{
					"name": "group2",
					"color": "#FFE035",
					"border": "#FFD803"
				},
				{
					"name": "group3",
					"color": "#89D803",
					"border": "#71CE47"
				},
				{
					"name": "group4",
					"color": "#FE6262",
					"border": "#F83B3B"
				},
				{
					"name": "group5",
					"color": "#2DACFF",
					"border": "#039BFF"
				},
				{
					"name": "group6",
					"color": "#FFA200",
					"border": "#F29A01"
				}
			]
		},
		"searchService": {
			"visible": false,
			"showDevConsoleOnVisible": true,
			"active": true,
			"name": "searchService",
			"html": "$servicesRoot/search/search.html",
			"file": "$servicesRoot/search/searchService.js",
			"category": "system"
		},
		"dockingService": {
			"visible": false,
			"showDevConsoleOnVisible": true,
			"active": true,
			"name": "dockingService",
			"html": "$servicesRoot/docking/docking.html",
			"file": "$servicesRoot/docking/dockingService.js",
			"config": {
				"BUFFER_SIZE": 20,
				"RESIZE_EVENT_THROTTLE_PERIOD": 0,
				"MINIMUM_WIDTH": 98,
				"MINIMUM_HEIGHT": 28,
				"GROUP_MODE": {
					"enabled": true,
					"behavior": "explicit"
				},
				"DEBUG": true,
				"ALLOW_GROUPS_TO_SNAP": true,
				"SNAPPING_OPACITY": 0.8
			},
			"category": "system"
		},
		"authenticationService": {
			"visible": false,
			"showDevConsoleOnVisible": true,
			"active": true,
			"name": "authenticationService",
			"html": "$servicesRoot/authentication/authentication.html",
			"file": "$servicesRoot/authentication/authenticationService.js",
			"category": "system"
		},
		"assimilationService": {
			"useWindow": true,
			"showDevConsoleOnVisible": true,
			"name": "assimilationService",
			"visible": false,
			"active": true,
			"html": "$servicesRoot/assimilation/assimilation.html",
			"file": "$servicesRoot/assimilation/assimilationService.js",
			"category": "system"
		},
		"finsembleNativeService": {
			"useWindow": true,
			"showDevConsoleOnVisible": true,
			"name": "finsembleNativeService",
			"visible": false,
			"active": true,
			"html": "$servicesRoot/finsembleNative/finsembleNative.html",
			"file": "$servicesRoot/finsembleNative/finsembleNativeService.js",
			"category": "system"
		},
		"RPCService": {
            "useWindow": true,
            "active": true,
            "name": "RPCService",
            "file": "$servicesRoot/RPC/RPCService.js",
            "visible": false,
            "html": "$servicesRoot/RPC/RPC.html",
            "component": {
                "type": "service"
            },
            "foreign":{
                "components":{
                    "Window Manager": false
                }
            },
            "window": {
                "preload": "$moduleRoot/FSBL.js"
            }
		},
		"hotkeyService": {
			"visible": false,
			"showDevConsoleOnVisible": true,
			"active": true,
			"name": "hotkeyService",
			"html": "$servicesRoot/hotkeys/hotkeys.html",
			"file": "$servicesRoot/hotkeys/hotkeysService.js",
			"category": "system"
		}
	}
}